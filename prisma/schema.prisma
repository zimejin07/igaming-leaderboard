generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Player {
  id        String   @id @default(cuid())
  name      String   @db.VarChar(100)
  score     Int
  updatedAt DateTime @updatedAt
  createdAt DateTime @default(now())
}
